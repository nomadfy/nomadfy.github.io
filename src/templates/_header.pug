header.header
  .header__overlay
    a.header__logo.hover__min(href="/")
      img(src="/assets/logo.png", alt="logo")
    .header__items
      a.navbar__icon(href="https://www.facebook.com" target="_blank")
        svg
          use(xlink:href="#icon-facebook")
      a.navbar__icon(href="https://www.instagram.com/" target="_blank" )
        svg
          use(xlink:href="#icon-instagram")
      a.navbar__icon(href="https://www.telegram.org/?setln=pt-br" target="_blank")
        svg
          use(xlink:href="#icon-telegram")
      a.navbar__icon(href="https://www.tiktok.com/pt-BR/" target="_blank")
        svg
          use(xlink:href="#icon-tiktok")
      a.navbar__icon(href="https://twitter.com/login?lang=pt" target="_blank")
        svg
          use(xlink:href="#icon-twitter")
    .button__block
      a.button.hover__min.button--login(href="/details/index.pug") ENTRAR
